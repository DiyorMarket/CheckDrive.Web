<ejs-sidebar id="sidebar" width="290px" enableDock dockSize="100px"
             target=".dock-target" enableGestures="false">
    <e-content-template>

        <div class="app-logo logo-wrapper">
            <img src='~/Images/LogoCheakDrive.png' />
        </div>    
        
        <div class="profile-wrapper">
            <i class="fa-solid fa-user"></i>
            <div class="titles d-flex flex-column ps-3">
                <h6 class="mb-0">@TempData["UserName"]</h6>
            </div>
        </div>

        <ejs-menu id="menu"
                  items="menuItems"
                  orientation="Vertical"
                  hamburgerMode></ejs-menu>
    </e-content-template>
</ejs-sidebar>