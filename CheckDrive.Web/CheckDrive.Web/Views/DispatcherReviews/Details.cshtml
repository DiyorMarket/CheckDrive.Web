        </div>
    </div>
    <div class="row value">
        <div class="col text-center">
            @Model.CarName
        </div>
        <div class="col text-center">
            @Model.CarMeduimFuelConsumption
        </div>
        <div class="col text-center">
            @Model.PouredFuel
        </div>
    </div>
    <div class="row content">
        <div class="col text-center">
            Ro'yxatga olingan vaqt
        </div>
        <div class="col text-center">
            Bosib o'tilgan masofa
        </div>
        <div class="col text-center">
            Sarflangan yoqilg'i
        </div>
    </div>
    <div class="row value">
        <div class="col text-center">
            @(Model.Date.ToString("yyyy-MM-dd | HH:mm"))
        </div>
        <div class="col text-center">
            @Model.DistanceCovered
        </div>
        <div class="col text-center">
            @Model.FuelSpended
        </div>
    </div>
    <div class="row content">
        <div class="col text-center">
            Boshlang`ich yoqilg`i qoldig`i
