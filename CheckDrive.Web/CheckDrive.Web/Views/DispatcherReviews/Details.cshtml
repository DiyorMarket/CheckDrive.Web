            @Model.DispatcherName
        </div>
        <div class="col text-center">
            @Model.DriverName
        </div>
    </div>
    <div class="row content">
        <div class="col text-center">
            Mexanik F.I.SH
        </div>
        <div class="col text-center">
            Operator F.I.SH
        </div>
    </div>
    <div class="row value">
        <div class="col text-center">
            @Model.MechanicName
        </div>
        <div class="col text-center">
            @Model.OperatorName
        </div>
    </div>
    <div class="row content">
        <div class="col text-center">
            Avtomobil
        </div>
        <div class="col text-center">
